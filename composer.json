{
    "name": "wuwx/laravel-hashing-sm3",
    "keywords": ["laravel", "hashing", "sm3", "hasher"],
    "type": "library",
    "config": {
        "allow-plugins": false
    },
    "require": {
        "ch4o5/sm3-php": "^1.1.6",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Wuwx\\LaravelHashingSm3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wuwx\\LaravelHashingSm3\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wuwx\\LaravelHashingSm3\\Sm3ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "wuwx",
            "email": "wuweixin@gmail.com"
        }
    ]
}
